{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "moduleResolution": "NodeNext",
    "outDir": "dist",
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "isolatedModules": true,
    "esModuleInterop": true,
    "declaration": true,
    "sourceMap": true,
    "strict": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
